@startuml
hide <<Path>> circle
hide <<Response>> circle
hide <<Parameter>> circle
hide empty methods
hide empty fields
set namespaceSeparator none

class "Pet" {
}

class "PetId" {
  {field} id : integer
}

class "NewPet" {
  {field} name : string
  {field} tag : string {O}
}

class "Error" {
  {field} code : integer
  {field} message : string
}

class "findPets" <<Path>> {
  {field} tags : string[] {O}
  {field} limit : integer {O}
}

class "findPets 200" <<Response>> {
}

class "findPets" <<Path>> {
}

class "find pet by id" <<Path>> {
  {field} id : integer
}

class "find pet by id" <<Path>> {
  {field} id : integer
}

class "find pet by id 204" <<Response>> {
}

"Pet" --> "1" "NewPet"

"Pet" --> "1" "PetId"

"findPets 200" --> "*" "Pet"

"findPets" ..> "1" "findPets 200"  :  "200"

"findPets" ..> "1" "Error"  :  "default"

"findPets" ..> "1" "Pet"  :  "200"

"findPets" ..> "1" "Error"  :  "default"

"findPets" --> "1" "NewPet"

"find pet by id" ..> "1" "Pet"  :  "200"

"find pet by id" ..> "1" "Error"  :  "default"

"find pet by id" ..> "1" "find pet by id 204"  :  "204"

"find pet by id" ..> "1" "Error"  :  "default"

@enduml